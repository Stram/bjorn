{
  "name": "project-oscar",
  "version": "1.0.0",
  "description": "Dashboard",
  "main": "src/index.ts",
  "scripts": {
    "install": "npm install",
    "start": "concurrently --kill-others --prefix \"[{name}]\" --names \"NODE,TYPESCRIPT\" --prefix-colors \"cyan.bold,yellow.bold\" \"npm run server:watch\" \"npm run server:transform-watch\"",
    "server:watch": "nodemon --watch dist dist/index.js",
    "server:transform": "tsc",
    "server:transform-watch": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.15.3",
    "typescript": "^2.4.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.0.36",
    "@types/winston": "^2.3.3",
    "concurrently": "^3.4.0"
  }
}
