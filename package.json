{
  "name": "project-oscar",
  "version": "1.0.0",
  "description": "Dashboard",
  "main": "src/index.ts",
  "scripts": {
    "install": "npm install",
    "server:bundle:watch": "webpack --config build-config/webpack.server.config.js -w",
    "server:bundle:build": "webpack --config build-config/webpack.server.config.js",
    "server:watch": "nodemon --watch dist dist/server.js",
    "server:start": "concurrently --kill-others --prefix \"[{name}]\" --names \"WEBPACK,NODEMON\" --prefix-colors \"blue.bold,green.bold\" \"npm run server:bundle:watch\" \"sleep 7 && npm run server:watch\"",
    "client:watch": "webpack --config build-config/webpack.client.config.js -w",
    "client:build": "webpack --config build-config/webpack.client.config.js",
    "client:start": "concurrently --kill-others --prefix \"[{name}]\" --names \"WEBPACK\" --prefix-colors \"blue.bold\" \"npm run client:watch\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.15.3",
    "express-handlebars": "^3.0.0",
    "vue": "^2.3.4",
    "vue-class-component": "^5.0.2",
    "vue-server-renderer": "^2.3.4",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.0.36",
    "@types/express-handlebars": "0.0.29",
    "@types/winston": "^2.3.3",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-loader": "^0.4.5",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "ts-loader": "^2.2.2",
    "typescript": "^2.4.1",
    "vue-loader": "^13.0.0",
    "vue-template-compiler": "^2.3.4",
    "webpack": "^3.1.0",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
